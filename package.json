{
  "name": "vp-kuljetus-transport-management",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build-prod": "node infisical.js >> .env.production && npm run dev",
    "start": "eval $(grep '^INFISICAL_SECRET_PATH' .env) && infisical run --path $INFISICAL_SECRET_PATH -- npm run dev",
    "dev": "tsc --noEmit --skipLibCheck && vite --open",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "3.8.9",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@fontsource/roboto": "5.0.8",
    "@mui/icons-material": "5.15.4",
    "@mui/material": "5.15.4",
    "@mui/x-data-grid": "6.19.3",
    "@tanstack/react-router": "1.15.13",
    "@tanstack/router-devtools": "1.4.6",
    "@tanstack/router-vite-plugin": "1.4.6",
    "envalid": "8.0.0",
    "graphql": "16.8.1",
    "i18next": "23.7.16",
    "i18next-browser-languagedetector": "7.2.0",
    "jotai": "2.6.4",
    "keycloak-js": "23.0.5",
    "material-ui-popup-state": "5.0.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.50.0",
    "react-i18next": "14.0.0"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "1.0.0",
    "@biomejs/biome": "1.5.2",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "@vitejs/plugin-react-swc": "3.5.0",
    "typescript": "5.2.2",
    "vite": "5.0.8",
    "vite-plugin-graphql-codegen": "3.3.6"
  }
}
